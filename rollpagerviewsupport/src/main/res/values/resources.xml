<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RollViewPager">
        <attr name="hint_mode">
            <enum name="point" value="0" />
            <enum name="number" value="1" />
        </attr>
        <attr name="hint_gravity">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <!--1 ,2 ,3-->
        <!--4 ,5, 6-->
        <!--7 ,8, 9-->
        <attr name="hint_locationNine" format="integer"/>
        <attr name="content_autoPlay" format="boolean" />
        <attr name="content_autoPlayInterval" format="integer" />
        <attr name="hint_marginRight" format="dimension" />
        <attr name="hint_marginLeft" format="dimension" />
        <attr name="hint_marginTop" format="dimension" />
        <attr name="hint_marginBottom" format="dimension" />
        <attr name="hint_color" format="color" />
        <attr name="hint_alpha" format="integer" />
        <attr name="selected_dot_res" format="reference" />

    </declare-styleable>

    <string name="app_name">RollPagerViewSupport</string>

</resources>